<div class="modal-body">
    <div class="col-md-4 col-md-offset-4 setEmail" id="changepwd">
      <h5 class="text-center">Change Password</h5>
      <form action="{{req.RequestURI}}" class="col-md-12 col-xs-12" method="POST">
        <input class="userMsg col-md-12 col-xs-12" name="Password" type="password" id="changepwd" placeholder="New Password" />
        <input type="password" id="comfirmPwd" name="RepeatPassword" placeholder="Comfirm New Password"  class="userMsg col-md-12 col-xs-12" />                    
        <div class="col-md-12 col-xs-12 login_btn">
          <button class="btn btn-yellow emailBtn" data-dismiss="modal" id="loginbtn">Save Changes</button>
        </div>     
      </form>     
    </div>
</div>
