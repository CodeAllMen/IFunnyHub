<!-- // videos content -->
  <div class="title">                         
      {{ render_tags req }}
      <p class="text-right font_gray">Approximately {{.total_count}} results</p>  
  </div>
  <div class="content grid" id="videos_bottom">
    {{ range .items }}
      {{render_item .}}
    {{end}}
  </div>
  <!-- turn page in videos page   -->
    {{ $req := req }}
    {{ $values := urlValues }}
    {{ $current_page := .page }}
    {{ $path := Sprintf "%s?%s" $req.URL.RawPath $values.Encode }}

    {{ $pagenation := render_pagenation .total_count .per_page .page }}
    <div class="pager col-xs-12">        
      {{ if $pagenation.HasPrev }}
        {{ $PrePage := Sprintf "%d"  $pagenation.PrevNUM }}
        <a href="{{ addURLQuery $path "page" $PrePage }}" name="prev" id="index_prev"><i class="fa fa-caret-left"></i></a>
      {{ end }}
      {{ range $pagenation.IterPages 1 1 3 1}}
      {{if eq . 0}}
      <span>...</span>
      {{ else }}
      {{ $page := Sprintf "%d" . }}
      <span name="nav" id="index_nav" class="pagenum" style="cursor: pointer;">
          <a class="navi {{if eq $current_page .}}page_selected {{end}}" href="{{ addURLQuery $path "page" $page }}" page_selected><span class="page_num">{{.}}</span></a>
      </span>
      {{ end }}
      {{ end }}
      {{ if $pagenation.HasNext }}
        {{ $NextPage := Sprintf "%d"  $pagenation.NextNUM }}
        <a href="{{ addURLQuery $path "page" $NextPage }}" name="next" id="index_next"  class="" style="cursor: pointer;"><i class="fa fa-caret-right"></i></a>
      {{ end }}
      <input name="goNum" id="goNumIndex" class="cssTxt hide" type="text" size="1" />
    </div>
